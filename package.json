{
   "name": "typeorm-express-starter-kit",
   "version": "0.0.1",
   "description": "A TypeScript + TypeORM + Express Starter Kit",
   "main": "index.js",
   "scripts": {
      "build": "tsc",
      "dev": "set debug=* && ts-node-dev --respawn --transpileOnly ./src/server.ts",
      "start": "node ./build/server.js",
      "prod": "npm run build && npm run start",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
   },
   "author": "Omar Cruz <omar@lesi.app>",
   "license": "ISC",
   "dependencies": {
      "bcryptjs": "^2.4.3",
      "body-parser": "^1.19.0",
      "class-validator": "^0.12.2",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^3.22.0",
      "jsonwebtoken": "^8.5.1",
      "lodash": "^4.17.19",
      "pg": "^8.2.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.25"
   },
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/body-parser": "^1.19.0",
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.6",
      "@types/helmet": "0.0.47",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/node": "^14.0.1",
      "nodemon": "^2.0.4",
      "ts-node": "8.10.1",
      "ts-node-dev": "^1.0.0-pre.44",
      "typescript": "3.9.2"
   }
}
